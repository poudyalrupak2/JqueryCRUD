    <meta name="author" content="">
    <title>
        Simple crud using modal popup and jquery
    </title>
    <!-- Custom fonts for this template-->
    <link href="~/Spadmin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="~/Spadmin/css/sb-admin-2.css" rel="stylesheet">
    <style>

        hr {
            margin-top: 0rem;
            margin-bottom: 0rem;
        }
    </style>
</head>
