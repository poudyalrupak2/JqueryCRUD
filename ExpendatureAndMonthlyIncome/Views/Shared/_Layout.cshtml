        <!-- End of Main Content -->
        <!-- Footer -->
        <hr style="border-top: 1px solid red;" />
        <footer class="sticky-footer bg-white">

            <div class=" my-auto">
                <div class="copyright text-center my-auto">
                    <span style="color:white">Copyright &copy; debugsoft</span>
                </div>
            </div>
        </footer>
