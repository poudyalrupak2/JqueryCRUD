                    <!-- Divider -->

                    @if (Session["category"].ToString() == "Admin")
                    {
                        <li class="nav-item active">
                            <a class="nav-link" href="@Url.Action("ShowProduct","Income")">
                                <i class="fas fa-fw fa-tachometer-alt"></i>
                                <span>Income</span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a class="nav-link" href="@Url.Action("Index","productdata")">
                                <i class="fas fa-fw fa-tachometer-alt"></i>
                                <span>Product</span>
                            </a>
                        </li>




                    }
                    else
                    {
                        <li class="nav-item active">
                            <a class="nav-link" href="@Url.Action("Logout","Account")">
                                <i class="fas fa-fw fa-tachometer-alt"></i>
                                <span>Logout</span>
                            </a>
                        </li>
                    }
                    <!-- Divider -->
                    <!-- Heading -->

                </ul>

            </div>
