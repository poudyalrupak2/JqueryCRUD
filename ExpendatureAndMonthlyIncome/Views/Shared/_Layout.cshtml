    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <!-- Bootstrap core JavaScript-->
    <script src="~/Spadmin/vendor/jquery/jquery.min.js"></script>
    <script src="~/Spadmin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/Spadmin/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/Spadmin/js/sb-admin-2.min.js"></script>
    <!-- Page level plugins -->
    <script src="~/vendor/chart.js/Chart.min.js"></script>
    <!-- Page level custom scripts -->
    <script src="~/Spadmin/js/demo/chart-area-demo.js"></script>
    <script src="~/Spadmin/js/demo/chart-pie-demo.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script type="text/javascript">

    $(document).ready(function() {
        $("#partialView").load('@Url.Content("~/AdminNav/Index")');
        $("#partialView1").load('@Url.Content("~/ClientNav/Index")');
